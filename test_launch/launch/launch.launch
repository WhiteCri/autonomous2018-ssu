<launch>
    <param name="use_sim_time" value="true"/>
    <include file="$(find laser_filters)/laser_filters.launch"/>
    <include file="$(find odometry)/launch/odometry.launch"/>
    <include file="$(find robot_pose_ekf)/robot_pose_ekf.launch"/>
    <node pkg="imu_maker" type="imu_maker" name="imu_maker"/>
    <include file="$(find platform_static_tf)/launch/robot_setup.launch"/>
    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping">
        <remap from="scan" to="scan_filtered"/>
    </node>
</launch>